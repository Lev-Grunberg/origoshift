    "@types/ip": "^1.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "jest": "^27.3.1",
    "jest-mock-extended": "^2.0.4",
    "ts-jest": "^27.0.7",
