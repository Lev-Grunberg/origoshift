{
  "name": "mediaserver",
  "version": "1.0.0",
  "description": "mediasoup server for inclubit 2",
  "main": "./dist/index.js",
  "types": "./dist/routers/appRouter.d.ts",
  "exports": {
    ".": {
      "types": "./dist/routers/appRouter.d.ts"
    }
  },
  "author": "Gunnar Oledal",
  "license": "ISC",
  "scripts": {
    "start": "dotenv -e ../../.env node ./dist/index.js",
    "dev": "dotenv -v DEVELOPMENT=true -e ../../.env -- node-dev ./src/index.ts",
    "build": "tsup-node --clean",
    "build:watch": "tsup-node --watch",
    "clean": "tsc --build --clean",
    "test": "jest --watch",
    "test:once": "jest"
  },
  "dependencies": {
    "database": "workspace:*",
    "debug-level": "3.0.0-1",
    "ip": "^1.1.8",
    "lodash": "^4.17.21",
    "schemas": "workspace:*",
    "superjson": "^1.12.2",
    "tiny-typed-emitter": "^2.1.0",
    "tozod": "^3.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.19.0"
  },
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "node-dev": "^7.4.3",
    "ts-node": "^10.4.0",
    "tscpaths": "^0.0.9"
  }
}
