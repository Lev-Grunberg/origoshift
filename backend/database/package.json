{
  "name": "database",
  "version": "1.0.0",
  "main": "index.ts",
  "types": "index.ts",
  "author": "Gunnar Oledal",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node ./seed.ts",
    "schema": "../database/prisma/schema.prisma"
  },
  "scripts": {
    "migrate:deploy": "dotenv -e ../../.env prisma migrate deploy",
    "db:generate": "prisma generate",
    "migrate": "dotenv -e ../../.env.dev prisma migrate dev",
    "migrate:reset": "dotenv -e ../../.env -- prisma migrate reset",
    "seed": "dotenv -e ../../.env prisma db seed",
    "studio": "dotenv -e ../../.env prisma studio",
    "dbpush": "dotenv -e ../../.env prisma db push"
  },
  "devDependencies": {
    "prisma": "3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@prisma/client": "3",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.0.1",
    "shared-types": "workspace:*"
  }
}
